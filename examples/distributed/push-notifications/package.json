{
  "name": "@naylence-examples/distributed-push-notifications",
  "version": "0.1.0",
  "description": "Naylence TypeScript example: distributed/push-notifications",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run rename-to-mjs && npm run fix-imports",
    "rename-to-mjs": "find dist -name '*.js' -exec sh -c 'mv \"$1\" \"${1%.js}.mjs\"' _ {} \\;",
    "fix-imports": "find dist -name '*.mjs' -exec sed -i '' 's/\\.js/\\.mjs/g' {} \\;",
    "sentinel": "node dist/sentinel.mjs",
    "push-sender": "node dist/push-sender.mjs",
    "push-receiver": "node dist/push-receiver.mjs",
    "client": "node dist/client.mjs",
    "clean": "rm -rf dist node_modules package-lock.json"
  },
  "dependencies": {
    "@naylence/agent-sdk": "^0.3.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.3"
  }
}
