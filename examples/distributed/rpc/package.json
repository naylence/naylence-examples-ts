{
  "name": "@naylence-examples/distributed-rpc",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && npm run rename-to-mjs && npm run fix-imports",
    "rename-to-mjs": "find dist -name '*.js' -exec sh -c 'mv \"$1\" \"${1%.js}.mjs\"' _ {} \\;",
    "fix-imports": "find dist -name '*.mjs' -exec sed -i '' 's/\\.js/\\.mjs/g' {} \\;",
    "client": "node dist/client.mjs",
    "sentinel": "node dist/sentinel.mjs",
    "math-agent": "node dist/math-agent.mjs"
  },
  "dependencies": {
    "@naylence/agent-sdk": "^0.3.3"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
