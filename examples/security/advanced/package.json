{
  "name": "@naylence-examples/security-advanced",
  "version": "0.1.0",
  "description": "Naylence TypeScript example: security/advanced",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run rename-to-mjs && npm run fix-imports",
    "rename-to-mjs": "find dist -name '*.js' -exec sh -c 'mv \"$1\" \"${1%.js}.mjs\"' _ {} \\;",
    "fix-imports": "find dist -name '*.mjs' -exec sed -i '' 's/\\.js/\\.mjs/g' {} \\;",
    "client": "node dist/client.mjs",
    "sentinel": "node dist/sentinel.mjs",
    "math-agent": "node dist/math-agent.mjs"
  },
  "dependencies": {
    "@naylence/advanced-security": "file:../../../../naylence-advanced-security-ts/naylence-advanced-security-0.3.15.tgz",
    "@naylence/agent-sdk": "file:../../../../naylence-agent-sdk-ts/naylence-agent-sdk-0.3.11.tgz"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.3",
    "why-is-node-running": "^3.2.2"
  }
}
